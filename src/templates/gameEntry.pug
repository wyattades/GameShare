- const running = self.status === 'running';
tr
  th= 'No Name'
  td= new Date(self.created_on).toLocaleString()
  td= self.status
  td
    .game-actions.field.has-addons
      if !running
        .control
          a.button
            span.icon.is-small
              i.fa.fa-play
      else
        .control
          a.button
            span.icon.is-small
              i.fa.fa-stop
      .control
        a.button
          span.icon.is-small
            i.fa.fa-pencil
          //- span Edit
      .control
        a.button
          span.icon.is-small
            i.fa.fa-trash
          //- span Delete